lot as plt\n","import numpy as np\n","#pandas로 csv파일 읽어들이기\n","import io\n","import pandas as pd\n","data = pd.read_csv(io.BytesIO(myfile['data_ver1.csv']))\n","#data를 넘파이 배열로\n","data=np.array(data, dtype=np.int) #데이터 타입은 정수형\n","#잘 불러들어왔는지 확인\n","print(data.shape)\n","data[:5]"],"execution_count":17,"outputs":[{"output_type":"stream","text":["(35, 3)\n"],"name":"stdout"},{"output_type":"execute_result","data":{"text/plain":["array([[485, 519, 503],\n","       [499, 501, 490],\n","       [506, 494, 508],\n","       [514, 526, 519],\n","       [501, 509, 493]])"]},"metadata":{"tags":[]},"execution_count":17}]},{"cell_type":"code","metadata":{"id":"T3Kuz82QzL0a","executionInfo":{"status":"ok","timestamp":1607437673486,"user_tz":-540,"elapsed":8783,"user":{"displayName":"성균관대학교박병준","photoUrl":"","userId":"16195189156410273961"}}},"source":["class Neuron:\n","  def __init__(self): #w와 b 초기화 작업\n","    self.w = 1.0\n","    self.b = 1.0\n","  \n","  def forpass(self, x): #y_hat 계산 메서드\n","    y_hat = x * self.w + self.b\n","    return y_hat\n","  \n","  def backprop(self, x, err): #w와 b를 업데이트하기 위한 w_grad, b_grad 계산\n","    w_grad = x*err\n","    b_grad = 1*err\n","    return w_grad, b_grad\n","\n","  def fit(self, x, y, epochs): #실제 훈련 과정\n","    for i in range(epochs): #에포크 수만큼 반복 훈련\n","      for x_i, y_i in zip(x, y): #모든 데이터 셋에 대해 반복\n","        y_hat = self.forpass(x_i)\n","        err = y_i - y_hat\n","        w_grad, b_grad = self.backprop(x_i, err)\n","\n","        #w와 b 업데이트(cost function의 값을 낮추고자)\n","        self.w += w_grad\n","        self.b += b_grad\n","  \n","def point(x, neuron):\n","  w = neuron.w\n","  b = neuron.b\n","  y = w*x + b\n","\n","  return (x, y)"],"execution_count":23,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":279},"id":"M8m1XHekzOtl","executionInfo":{"status":"ok","timestamp":1607437686287,"user_tz":-540,"elapsed":8909,"user":{"displayName":"성균관대학교박병준","photoUrl":"","userId":"161951