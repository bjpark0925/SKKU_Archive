n\n","x = diabetes.data[:, 2]\n","y = diabetes.target"],"execution_count":5,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":297},"id":"dziEm44QrWp4","executionInfo":{"status":"ok","timestamp":1607427182638,"user_tz":-540,"elapsed":1673,"user":{"displayName":"성균관대학교박병준","photoUrl":"","userId":"16195189156410273961"}},"outputId":"8d203b5e-9de3-4eb3-d9cc-6c9899fca88a"},"source":["neuron1 = Neuron()\n","neuron1.fit(x, y, 1) #에포크 수 = 1\n","\n","#훈련 완료 후, 임의의 두 x값을 설정해 직선 표현\n","plt.scatter(x, y)\n","p1 = point(-0.1, neuron1)\n","p2 = point(0.15, neuron1)\n","plt.plot([p1[0],p2[0]], [p1[1],p2[1]])\n","plt.xlabel('x')\n","plt.ylabel('y')\n","plt.show()\n","print(neuron1.w, neuron1.b)"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}},{"output_type":"stream","text":["587.8654539985689 99.40935564531424\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":297},"id":"A1GxDl8qtwgU","executionInfo":{"status":"ok","timestamp":1607427182641,"user_tz":-540,"elapsed":1662,"user":{"displayName":"성균관대학교박병준","photoUrl":"","userId":"16195189156410273961"}},"outputId":"4ad2a551-ab93-4383-c24b-0f2070004f8e"},"source":["neuron10 = Neuron()\n","neuron10.fit(x, y, 10) #에포크 수 = 10\n","\n","#훈련 완료 후, 임의의 두 x값을 설정해 직선 표현\n","plt.scatter(x, y)\n","p1 = point(-0.1, neuron10)\n","p2 = point(0.15, neuron10)\n","plt.plot([p1[0],p2[0]], [p1[1],p2[1]])\n","plt.xlabel('x')\n","plt.ylabel('y')\n","plt.show()\n","print(neuron10.w, neuron10.b)"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}},{"output_type":"stream","text":["913.5656499923714 123.39181064719298\n"],"name":"stdout"}]},{"cell_type":"code","metadata":